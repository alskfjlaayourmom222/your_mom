#include <stdio.h>

void Sum1(){

  int n;scanf("%d",&n);
  int sum=0;
  for(int i=1;i<=n;++i){
    sum+=i;
      }
  
  printf("%d",sum);
  }
void Sum2(int a){
  int sum=0;
  for(int i=1;i<=a;++i){
    sum +=i;
    }
    printf("%d",sum);
}
int Sum3(){
  int n;scanf("%d",&n);
  int sum=0;
  for(int i=1; i<=n; ++i){
    sum+=i;
    }
  return sum;
}
int Sum4(int a){
  int sum =0;
  for(int i=1;i<=a;++i){
    sum+=i;
  }
  return sum;
}
  
int main(void) {
  int n;scanf("%d",&n);
  Sum1();
  Sum2(n);
  printf("%d\n",Sum3());
  printf("%d\n",Sum4(n));
  
  return 0;
}
