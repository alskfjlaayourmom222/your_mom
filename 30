#include <stdio.h>
int main(void)
{
    int n;scanf("%d",&n);
    switch (n)
        {
            case 1:
            case 8:
            case 15:
            case 22:
            case 29:
            printf("토요일");
            break;
            case 2:
            case 9:
            case 16:
            case 23:
            case 30:        
            printf("부활");
            break; 
            case 3:
            case 10:
            case 17:
            case 24:
            case 31:
            printf("공포의 월요일");
            break; 
            case 4:
            case 11:
            case 18:
            case 25:
            printf("절망의 화요일");
            break; 
            case 5:
            case 12:
            case 19:
            case 26:
            printf("이름이 멋있는 수요일");
            break; 
            case 6:
            case 13:
            case 20:
            case 27:
            printf("목요일");
            break; 
            case 7:
            case 14:
            case 21:
            case 28:
            printf("금요일");
            break; 
                
            
            default:
                printf("고인");
                break;                
            }
    return 0;
    }
